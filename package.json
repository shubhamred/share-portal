{
  "name": "klub-portal",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run tests"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less|scss)$": "identity-obj-proxy",
      "^app/(.*)$": "<rootDir>src/js/$1"
    },
    "collectCoverageFrom": [
      "src/js/**/*.{js,jsx}",
      "tests/**/*.{js,jsx}",
      "!tests/mocked/*.{js,jsx}",
      "!tests/cucumber/**/*",
      "!tests/mocked/**/*",
      "!src/js/**/*.story*{js,jsx}"
    ],
    "coverageDirectory": "./reports/coverage"
  },
  "scripts": {
    "postinstall": "patch-package",
    "clean": "del ./dist/*",
    "format": "prettier --write ./src/**/*.{js,jsx,ts,tsx}",
    "tests": "cross-env run-p eslint test",
    "test": "cross-env jest -i",
    "reports": "run-p eslint-report coverage-summary",
    "eslint": "cross-env eslint './src/**/*.{js,jsx,ts,tsx}'",
    "eslint-json-summary": "npm run eslint -- -f json > ./reports/eslint.json",
    "eslint-report": "cross-env eslint './src/**/*.{js,jsx,ts,tsx}' -f node_modules/eslint-html-reporter/reporter.js -o reports/eslint.html",
    "coverage": "cross-env jest -i --coverage",
    "coverage-summary": "cross-env istanbul report --root ./ --include reports/coverage/coverage-final.json --dir ./reports json-summary",
    "storybook": "start-storybook -p 9001",
    "serve": "cross-env NODE_CONFIG_DIR=./config/env webpack serve --config ./webpack.config.js",
    "build": "npm run clean && cross-env NODE_CONFIG_DIR=./config/env NODE_ENV=production webpack --config webpack.config.production.js"
  },
  "author": "",
  "license": "ISC",
  "homepage": "https://bitbucket.org/klub-tech/back-office-ui#readme",
  "dependencies": {
    "@auth0/auth0-spa-js": "^1.17.0",
    "@babel/polyfill": "^7.12.1",
